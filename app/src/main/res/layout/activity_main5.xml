        android:inputType="text"
        android:padding="16dp"
        android:textColor="@color/textColor"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="335dp"
        tools:layout_editor_absoluteY="554dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Max"
        android:textColor="@color/textColor"
        android:textSize="29dp"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="170dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="% (+/-)"
        android:textColor="@color/textColor"
        android:textSize="29dp"
        tools:layout_editor_absoluteX="350dp"
        tools:layout_editor_absoluteY="170dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Congratulations *User*,
                                                      You've completed a training cycle,
                                     here is how you did"
        android:textAlignment="center"
        android:textColor="@color/textColor"
        android:textSize="21dp"

        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="742dp" />

    <TextView
        android:layout_width="486dp"
        android:layout_height="124dp"
        android:background="@color/white"
        android:text="So each week, where for each life you do a percentage of
your current max, Week 1: 50% Week 2: 60%
Week 3: 70% Week 4: 80%. Every lift you will alternate
between upper and lower body exercises You will then
do a 1 rep max for Bench, Squat, and Deadlift.
Repeat this cycle each month and you are guaranteed to
 meet your goals!!!!"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="-3dp"
        tools:layout_editor_absoluteY="850dp" />

    <ImageView
        android:layout_width="121.5dp"
        android:layout_height="124dp"
