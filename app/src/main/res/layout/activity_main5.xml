        tools:layout_editor_absoluteX="333dp"
        tools:layout_editor_absoluteY="0dp" />

    <EditText
        android:id="@+id/CurrentBench"
        android:layout_width="138dp"
        android:layout_height="63dp"
        android:background="@color/black"
