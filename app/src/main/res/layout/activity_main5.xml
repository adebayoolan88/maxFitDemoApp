        android:inputType="text"
        android:padding="16dp"
        android:textColor="@color/textColor"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="166dp"
        tools:layout_editor_absoluteY="554dp" />

    <EditText
        android:id="@+id/CurrentCardio"
        android:layout_width="138dp"
        android:layout_height="63dp"
        android:background="@color/black"
        android:hint="Enter your gender"
        android:inputType="text"
        android:padding="16dp"
        android:textColor="@color/textColor"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="166dp"
        tools:layout_editor_absoluteY="660dp" />

    <EditText
        android:id="@+id/GoalCardio"
        android:layout_width="138dp"
        android:layout_height="63dp"
        android:background="@color/black"
        android:hint="Enter your goal cardio"
        android:inputType="text"
        android:padding="16dp"
        android:textColor="@color/textColor"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="335dp"
        tools:layout_editor_absoluteY="660dp" />



    <TextView
        android:id="@+id/BenchLabel"
        android:layout_width="143dp"
        android:layout_height="71dp"
        android:text="Bench"
        android:textAlignment="center"
        android:textColor="@color/textColor"
        android:textSize="38dp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="221dp" />

    <TextView
        android:id="@+id/SquatLabel"
        android:layout_width="143dp"
        android:layout_height="71dp"
        android:text="Squat"
        android:textAlignment="center"
        android:textColor="@color/textColor"
        android:textSize="38dp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="328dp" />

    <TextView
        android:id="@+id/DeadliftLabel"
        android:layout_width="143dp"
        android:layout_height="71dp"
