        android:textColor="@color/textColor"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="166dp"
        tools:layout_editor_absoluteY="221dp" />

    <EditText
        android:id="@+id/CurrentSquat"
        android:layout_width="138dp"
